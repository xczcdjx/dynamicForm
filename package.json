{
  "name": "dynamicformdjx",
  "version": "0.1.2",
  "type": "module",
  "author": "xczcdjx <xczcdjx@126.com>",
  "description": "Dynamic form for Vue3 (core or naive-ui or element-plus)",
  "repository": {
    "type": "git",
    "url": "https://github.com/xczcdjx/dynamicForm"
  },
  "homepage": "https://github.com/xczcdjx/dynamicForm#readme",
  "bugs": {
    "url": "https://github.com/xczcdjx/dynamicForm/issues"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./naiveUi": {
      "types": "./dist/naiveUi/index.d.ts",
      "import": "./dist/naiveUi/index.mjs",
      "require": "./dist/naiveUi/index.cjs"
    },
    "./elementPlus": {
      "types": "./dist/elementPlus/index.d.ts",
      "import": "./dist/elementPlus/index.mjs",
      "require": "./dist/elementPlus/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "keywords": [
    "vite",
    "vue3",
    "typescript",
    "dynamicForm"
  ],
  "scripts": {
    "devTest": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "pub": "npm version patch && npm publish"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {

  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vue/tsconfig": "^0.8.1",
    "element-plus": "^2.11.8",
    "less": "^4.4.2",
    "naive-ui": "^2.43.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vue": "^3.0.0",
    "vue-tsc": "^3.1.3"
  }
}
